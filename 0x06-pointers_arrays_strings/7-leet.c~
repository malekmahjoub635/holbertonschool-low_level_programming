#include "holberton.h"
/**
 *leet -  a function that encodes a string into 1337
 * 
 *Return: 0
 */
char *leet(char *s)
{
int i, j;
char *c[5] = {'aA','eE','oO','tT','lL'};
char *l[5] = {'44', '33', '00', '77', '11'};
for (i = 0; s[i] != '\0'; i ++)
for (j = 0; j < 10; j++)
if (s[i] == c[j])
{
s[i] = l[j];
}
}
